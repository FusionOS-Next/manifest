<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <!-- Github -->
  <remote  name="github"
           fetch="https://github.com/" />

  <!-- fos-next -->
  <remote  name="fos"
           fetch="https://github.com/Fusionos-next/"
           revision="14" />

  <!-- Rootdir -->
  <project path="bionic" name="bionic" groups="pdk" remote="fos" />
  <project path="manifest" name="manifest" remote="fos" />

  <!-- Bootable -->
  <project path="bootable/recovery" name="bootable_recovery" groups="pdk" remote="fos" />

  <!-- Build -->
  <project path="build/make" name="build" groups="pdk,sysui-studio" remote="fos" >
    <linkfile dest="build/CleanSpec.mk" src="CleanSpec.mk" />
    <linkfile dest="build/buildspec.mk.default" src="buildspec.mk.default" />
    <linkfile dest="build/core" src="core" />
    <linkfile dest="build/envsetup.sh" src="envsetup.sh" />
    <linkfile dest="build/target" src="target" />
    <linkfile dest="build/tools" src="tools" />
  </project>
  <project path="build/soong" name="build_soong" groups="pdk,tradefed" remote="fos" >
    <linkfile dest="Android.bp" src="root.bp" />
    <linkfile dest="bootstrap.bash" src="bootstrap.bash" />
  </project>

  <!-- Device -->
  <project path="device/custom/sepolicy" name="device_custom_sepolicy" remote="fos" />
  <!-- <project path="device/qcom/sepolicy-legacy-um" name="device_qcom_sepolicy-legacy-um" groups="qcom,sdm660" remote="fos" /> -->

  <!-- Frameworks -->
  <project path="frameworks/av" name="frameworks_av" groups="pdk" remote="fos" />
  <project path="frameworks/base" name="frameworks_base" groups="pdk-cw-fs,pdk-fs,sysui-studio" remote="fos" />
  <project path="frameworks/native" name="frameworks_native" groups="pdk" remote="fos" />
  <project path="frameworks/opt/telephony" name="frameworks_opt_telephony" groups="pdk" remote="fos" />

  <!-- Packages -->
  <project path="packages/apps/Settings" name="packages_apps_Settings" groups="pdk-fs" remote="fos" />
  <!-- <project path="packages/modules/Bluetooth" name="packages_modules_Bluetooth" groups="pdk" remote="fos" /> -->
  <!-- <project path="packages/providers/DownloadProvider" name="packages_providers_DownloadProvider" groups="pdk-cw-fs,pdk-fs" remote="fos" /> -->
  <project path="packages/services/Telecomm" name="packages_services_Telecomm" groups="pdk-cw-fs,pdk-fs" remote="fos" />

  <!-- System -->
  <project path="system/core" name="system_core" groups="pdk" remote="fos" />
  <project path="system/sepolicy" name="system_sepolicy" groups="pdk" remote="fos" />
  <project path="system/update_engine" name="system_update_engine" groups="pdk" remote="fos" />

  <!-- Vendor -->
  <project path="vendor/aosp" name="vendor_aosp" remote="fos" />
  <project path="vendor/custom-preference" name="vendor_custom-preference" remote="fos" />
</manifest>